{
    "name": "cultuurnet/calendar-summary-v3",
    "description": "Library to convert cultuurnet dates to a readable summary",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
      {
        "name": "Publiq vzw",
        "email": "info@publiq.be"
      }
    ],
    "require": {
      "php": "^7.1",
      "ext-intl": "*",
      "delfimov/translate": "^2.4",
      "cultuurnet/search-v3": "dev-master",
      "psr/container": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "CultuurNet\\CalendarSummaryV3\\": ["src"]
      }
    },
    "autoload-dev": {
      "psr-4": {
        "CultuurNet\\CalendarSummaryV3\\Tests\\": "tests/"
      }
    },
    "require-dev": {
      "escapestudios/symfony2-coding-standard": "~2.0",
      "squizlabs/php_codesniffer": "2.6.1",
      "phing/phing": "~2.10",
      "satooshi/php-coveralls": "~0.7",
      "mikey179/vfsstream": "~1.6.2",
      "phpunit/phpunit": "^7.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
      "phing": "./vendor/bin/phing test"
    }
}
